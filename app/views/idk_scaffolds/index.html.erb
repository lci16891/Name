<p id="notice"><%= notice %></p>

<h1>Idk Scaffolds</h1>

<table>
  <thead>
    <tr>
      <th>Avatar</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @idk_scaffolds.each do |idk_scaffold| %>
      <tr>
        <td><%= idk_scaffold.avatar %></td>
        <td><%= idk_scaffold.user_id %></td>
        <td><%= link_to 'Show', idk_scaffold %></td>
        <td><%= link_to 'Edit', edit_idk_scaffold_path(idk_scaffold) %></td>
        <td><%= link_to 'Destroy', idk_scaffold, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Idk Scaffold', new_idk_scaffold_path %>
